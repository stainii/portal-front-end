{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/**/favicon.ico",
                    "/**/index.html",
                    "/**/*.css",
                    "/**/*.js",
                    "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani|json)"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/**/assets/**/*"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "front-end api",
            "urls": ["http*://**/front-end/api/module/**/*"],
            "cacheConfig": {
                "maxSize": 3,
                "maxAge": "1d",
                "timeout": "10s",
                "strategy": "performance"
            }
        },
        {
            "name": "notifications",
            "urls": ["http*://**/notifications/api/**/*"],
            "cacheConfig": {
                "maxSize": 10,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        },
        {
            "name": "health",
            "urls": ["http*://**/health/api/**/*"],
            "cacheConfig": {
                "maxSize": 10,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        },
        {
            "name": "housagotchi",
            "urls": ["http*://**/housagotchi/api/**/*"],
            "cacheConfig": {
                "maxSize": 10,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        },
        {
            "name": "todo",
            "urls": ["http*://**/todo/api/**/*"],
            "cacheConfig": {
                "maxSize": 10,
                "maxAge": "7d",
                "timeout": "10s",
                "strategy": "freshness"
            }
        }
    ]
}
